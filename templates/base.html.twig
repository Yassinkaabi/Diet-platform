<script>
			$(document).ready(function () {
				$('.comment-form').submit(function (e) {
				e.preventDefault(); // Prevent the default form submission

				var form = $(this);
				var url = form.attr('action');
				var method = form.attr('method');
				var data = form.serialize();

				// Make the AJAX request
				$.ajax({
				url: url,
				method: method,
				data: data,
				success: function (response) { // Display the success message and hide the form
				$('.comment-form').hide();
				$('.comment-success-message').show();
				setTimeout(function () {
				$('.comment-success-message').hide();
				}, 3000);
				},
				error: function (error) {
				console.log('Error:', error);
				}
				});
				});
			});

</script>

<script>
			function showForm() {
				var commentForm = document.getElementById('commentForm');
				var currentDisplay = commentForm.style.display;

				// If the form is currently visible, hide it; otherwise, show it
				commentForm.style.display = (currentDisplay === 'block' || currentDisplay === '') ? 'none' : 'block';
				}
			$(document).ready(function () {
					// Hide success messages after 5 seconds
					setTimeout(function () {
						$('.alert-success').hide();
					}, 3000);

					// Hide error messages after 5 seconds
					setTimeout(function () {
						$('.alert-danger').hide();
					}, 3000);
				});
